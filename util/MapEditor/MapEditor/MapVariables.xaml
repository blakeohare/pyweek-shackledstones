<Window x:Class="MapEditor.MapVariables"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dg="http://schemas.microsoft.com/wpf/2008/toolkit"
        Title="Map Variables" Height="312" Width="330">
    <Grid>
        
        <Button Content="Save Values" Height="23" HorizontalAlignment="Right" Margin="0,0,12,12" Name="SaveValues" VerticalAlignment="Bottom" Width="75" />
        <Button Content="Add Value" Height="23" HorizontalAlignment="Left" Margin="12,0,0,12" Name="AddValue" VerticalAlignment="Bottom" Width="75" />
        <ListBox Height="220" HorizontalAlignment="Left" Margin="12,12,0,0" Name="variables" VerticalAlignment="Top" Width="284" />
    </Grid>
</Window>
